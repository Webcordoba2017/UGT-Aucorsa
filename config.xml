    <icon density="ldpi" src="icono.png" />        
    <icon density="mdpi" src="icono.png" />        
    <icon density="hdpi" src="icono.png" />        
    <icon density="xhdpi" src="icono.png" />        
    <icon density="xxhdpi" src="icono.png" />        
